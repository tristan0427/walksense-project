{
  "appId": "com.walksense.app",
  "appName": "Walksense",
  "webDir": "dist",
  "plugin": {
    "Geolocation": {
      "enableHighAccuracy": true
    },
    "BackgroundGeoLocation": {
      "desiredAccuracy": 10,
      "distanceFilter": 10,
      "stopOnTerminate": false,
      "startOnBoot": true
    }
  },

  "server": {
    "cleartext": true
  },
  "android": {
    "allowMixedContent": true
  }
}
